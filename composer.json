    "authors": [
        {
            "name" : "Gent Islami https://github.com/gentislami"
        }
    ],
    "type": "shopware-platform-plugin",
    "version": "1.0.0",
    "require": {
        "shopware/core": "6.5.*"
    },
    "extra": {
        "shopware-plugin-class": "GentIslami\\OrderShippingWebhookReport\\OrderShippingWebhookReportPlugin",
        "label": {
            "en-GB": "Order Shipping Webhook Report"
        }
    },
    "autoload": {
        "psr-4": {
            "GentIslami\\OrderShippingWebhookReport\\": "src/"
        }
    }
}
